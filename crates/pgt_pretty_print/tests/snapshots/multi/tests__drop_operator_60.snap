CREATE OPERATOR === (
  PROCEDURE = int8eq,
  LEFTARG = pg_catalog.int8,
  RIGHTARG = pg_catalog.int8,
  COMMUTATOR = ===
);

CREATE OPERATOR !== (
  PROCEDURE = int8ne,
  LEFTARG = pg_catalog.int8,
  RIGHTARG = pg_catalog.int8,
  NEGATOR = ===,
  COMMUTATOR = !==
);

DROP OPERATOR !==(pg_catalog.int8, pg_catalog.int8);

SELECT
  ctid,
  oprcom
FROM
  pg_catalog.pg_operator fk
WHERE oprcom <> 0
  AND NOT EXISTS (SELECT
    1
  FROM
    pg_catalog.pg_operator pk
  WHERE pk.oid = fk.oprcom);

SELECT
  ctid,
  oprnegate
FROM
  pg_catalog.pg_operator fk
WHERE oprnegate <> 0
  AND NOT EXISTS (SELECT
    1
  FROM
    pg_catalog.pg_operator pk
  WHERE pk.oid = fk.oprnegate);

DROP OPERATOR ===(pg_catalog.int8, pg_catalog.int8);

CREATE OPERATOR <| (
  PROCEDURE = int8lt,
  LEFTARG = pg_catalog.int8,
  RIGHTARG = pg_catalog.int8
);

CREATE OPERATOR |> (
  PROCEDURE = int8gt,
  LEFTARG = pg_catalog.int8,
  RIGHTARG = pg_catalog.int8,
  NEGATOR = <|,
  COMMUTATOR = <|
);

DROP OPERATOR |>(pg_catalog.int8, pg_catalog.int8);

SELECT
  ctid,
  oprcom
FROM
  pg_catalog.pg_operator fk
WHERE oprcom <> 0
  AND NOT EXISTS (SELECT
    1
  FROM
    pg_catalog.pg_operator pk
  WHERE pk.oid = fk.oprcom);

SELECT
  ctid,
  oprnegate
FROM
  pg_catalog.pg_operator fk
WHERE oprnegate <> 0
  AND NOT EXISTS (SELECT
    1
  FROM
    pg_catalog.pg_operator pk
  WHERE pk.oid = fk.oprnegate);

DROP OPERATOR <|(pg_catalog.int8, pg_catalog.int8);
