SELECT
  c.oid AS view_id,
  n.nspname AS view_schema,
  c.relname AS view_name,
  r.ev_action AS view_definition
FROM
  pg_class c
  INNER JOIN
    pg_namespace n ON n.oid = c.relnamespace
  INNER JOIN
    pg_rewrite r ON r.ev_class = c.oid
WHERE c.relkind IN ('v', 'm');
