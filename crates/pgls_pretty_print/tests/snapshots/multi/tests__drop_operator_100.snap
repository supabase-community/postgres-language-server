create operator === (PROCEDURE = int8eq, LEFTARG = bigint, RIGHTARG = bigint, COMMUTATOR = ===);

create operator !== (PROCEDURE = int8ne,
LEFTARG = bigint,
RIGHTARG = bigint,
NEGATOR = ===,
COMMUTATOR = !==);

drop operator !== (bigint, bigint);

select
  ctid,
  oprcom
from
  pg_catalog.pg_operator as fk
where
  oprcom <> 0 and
  not exists
  (
    select
      1
    from
      pg_catalog.pg_operator as pk
    where
      pk.oid = fk.oprcom
  );

select
  ctid,
  oprnegate
from
  pg_catalog.pg_operator as fk
where
  oprnegate <> 0 and
  not exists
  (
    select
      1
    from
      pg_catalog.pg_operator as pk
    where
      pk.oid = fk.oprnegate
  );

drop operator === (bigint, bigint);

create operator <| (PROCEDURE = int8lt, LEFTARG = bigint, RIGHTARG = bigint);

create operator |> (PROCEDURE = int8gt,
LEFTARG = bigint,
RIGHTARG = bigint,
NEGATOR = <|,
COMMUTATOR = <|);

drop operator |> (bigint, bigint);

select
  ctid,
  oprcom
from
  pg_catalog.pg_operator as fk
where
  oprcom <> 0 and
  not exists
  (
    select
      1
    from
      pg_catalog.pg_operator as pk
    where
      pk.oid = fk.oprcom
  );

select
  ctid,
  oprnegate
from
  pg_catalog.pg_operator as fk
where
  oprnegate <> 0 and
  not exists
  (
    select
      1
    from
      pg_catalog.pg_operator as pk
    where
      pk.oid = fk.oprnegate
  );

drop operator <| (bigint, bigint);
