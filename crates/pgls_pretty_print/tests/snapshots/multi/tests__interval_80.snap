select
  lhst.i as lhs,
  rhst.i as rhs,
  lhst.i < rhst.i as lt,
  lhst.i <= rhst.i as le,
  lhst.i = rhst.i as eq,
  lhst.i > rhst.i as gt,
  lhst.i >= rhst.i as ge,
  lhst.i <> rhst.i as ne
from
  infinite_interval_tbl as lhst
  inner join
    infinite_interval_tbl as rhst
  on true
where
  not isfinite(lhst.i);

