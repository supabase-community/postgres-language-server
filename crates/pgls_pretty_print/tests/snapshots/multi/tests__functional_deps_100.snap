create temporary table products (
  product_id INT,
  name TEXT,
  price NUMERIC
);

create temporary table sales (
  product_id INT,
  units INT
);

select
  product_id,
  p.name,
  SUM(s.units) * p.price as sales
from
  products as p
  left outer join
    sales as s
  using ("product_id")
group by product_id,
  p.name,
  p.price;

select
  product_id,
  p.name,
