select cast('\377' as CHAR);

select cast(cast('a' as CHAR) as TEXT);

select cast(cast('\377' as CHAR) as TEXT);

select cast(cast('\000' as CHAR) as TEXT);

select cast(cast('a' as TEXT) as CHAR);

select cast(cast('\377' as TEXT) as CHAR);

