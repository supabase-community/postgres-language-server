select distinct
  cast(i || '/' || j as PG_LSN) as f
from
  generate_series(1, 10) as i,
  generate_series(1, 10) as j,
  generate_series(1, 5) as k
where
  i <= 10 and j > 0 and j <= 10
order by f;

