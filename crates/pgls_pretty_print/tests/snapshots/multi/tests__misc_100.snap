create table hobbies_r (
  name text,
  person text
);

create table equipment_r (
  name text,
  hobby text
);

insert into hobbies_r (name, person)
select
  'posthacking',
  p.name
from
  person as p
where
  p.name = 'mike' or p.name = 'jeff';

