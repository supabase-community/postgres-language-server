prepare q3
(text,
int,
double precision,
boolean,
smallint)
as select
  *
from
  tenk1
where
  string4 = $1 and
  (four = $2 or
  ten = cast($3 as bigint) or
  true = $4 or
  odd = cast($5 as int))
order by unique1;;

execute q3
('AAAAxx',
cast(5 as smallint),
cast(10.5 as double precision),
false,
cast(4 as bigint));

execute q3 ('bool');

execute q3
('bytea',
