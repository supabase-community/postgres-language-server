select
  r.*,
  false,
  array[resorigtbl]
from
  results as r
where
  true
union all
select
  view.view_id,
  view.view_schema,
  view.view_name,
  view.view_column,
  tab.resorigtbl,
  tab.resorigcol,
  tab.resorigtbl = any (path),
  path || tab.resorigtbl
from
  recursion as view
  inner join
    results as tab
  on view.resorigtbl = tab.view_id and
    view.resorigcol = tab.view_column
where
  not is_cycle;
