SELECT
  view_id,
  view_schema,
  view_name,
  resorigtbl,
  resorigcol,
  array_agg(attname) AS view_columns
FROM
  recursion
  INNER JOIN pg_attribute AS vcol
  ON vcol.attrelid =
    view_id AND
    vcol.attnum =
    view_column
GROUP BY view_id,
  view_schema,
  view_name,
  resorigtbl,
  resorigcol;
