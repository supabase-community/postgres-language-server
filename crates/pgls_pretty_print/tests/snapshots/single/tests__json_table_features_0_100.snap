create view jsonb_table_view2
as select
  *
from
  JSON_TABLE(
    cast('null' as JSONB),
    'lax $[*]'
    PASSING 1 + 2 as a,
    cast('"foo"' as JSON) as "b c"
    COLUMNS (
      int INT PATH '$',
      text text PATH '$',
      js JSON PATH '$',
      jsb jsonb PATH '$'
    )
    null on ERROR
  );
