create view jsonb_table_view2
as select
  *
from
  json_table(
    cast('null' as jsonb),
    'lax $[*]'
    passing 1 + 2 as a,
    cast('"foo"' as json) as "b c"
    columns (
      int int path '$',
      text text path '$',
      js json path '$',
      jsb jsonb path '$'
    )
    null on error
  );
