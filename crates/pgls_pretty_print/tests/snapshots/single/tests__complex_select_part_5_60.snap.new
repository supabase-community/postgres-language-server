SELECT
  r.*,
  FALSE,
  ARRAY[resorigtbl]
FROM
  results AS r
WHERE
  TRUE
UNION ALL
SELECT
  view.view_id,
  view.view_schema,
  view.view_name,
  view.view_column,
  tab.resorigtbl,
  tab.resorigcol,
  tab.resorigtbl = ANY (path),
  path || tab.resorigtbl
FROM
  recursion AS view
  INNER JOIN results AS tab
  ON view.resorigtbl =
    tab.view_id AND
    view.resorigcol =
    tab.view_column
WHERE
  NOT is_cycle;
